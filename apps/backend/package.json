{
  "name": "backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "start": "bun src/index.ts",
    "build": "bun build --compile --bytecode --minify --sourcemap --target=bun-linux-x64 --no-compile-autoload-dotenv ./src/index.ts --outfile=server"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.11.11"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clerk/backend": "^1.24.3",
    "hono": "^4.12.2",
    "ioredis": "^5.4.2",
    "pg": "^8.14.1"
  }
}